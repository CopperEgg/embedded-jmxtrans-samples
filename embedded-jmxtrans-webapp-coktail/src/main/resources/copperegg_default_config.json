{"config":
  { "metric_groups" :
    [
      {
        "name":"jvm_metrics",
        "label":"jvm Metrics",
        "frequency":15,
        "metrics":
          [
            {"type":"ce_gauge","name":"jvm.os.FreePhysicalMemorySize","unit":"bytes"},
            {"type":"ce_gauge","name":"jvm.os.FreeSwapSpaceSize","unit":"bytes"},
            {"type":"ce_gauge_f","name":"jvm.os.SystemLoadAverage"},
            {"type":"ce_gauge","name":"jvm.os.OpenFileDescriptorCount","unit":"descriptors"},
            {"type":"ce_gauge","name":"jvm.os.CommittedVirtualMemorySize","unit":"bytes"},
            {"type":"ce_gauge","name":"jvm.os.ProcessCpuTime","unit":"seconds"},
            {"type":"ce_counter","name":"jvm.runtime.Uptime","unit":"seconds"},
            {"type":"ce_gauge","name":"jvm.loadedClasses.LoadedClassCount","unit":"classes"},
            {"type":"ce_gauge","name":"jvm.thread.ThreadCount","unit":"threads"},
            {"type":"ce_gauge","name":"jvm.thread.DaemonThreadCount","unit":"threads"},
            {"type":"ce_gauge","name":"jvm.gc.Copy.CollectionCount","unit":"total collections"},
            {"type":"ce_gauge","name":"jvm.gc.Copy.CollectionTime","unit":"seconds"},
            {"type":"ce_gauge","name":"jvm.gc.MarkSweepCompact.CollectionCount","unit":"total collections"},
            {"type":"ce_gauge","name":"jvm.gc.MarkSweepCompact.CollectionTime","unit":"seconds"}
         ]
      },
      {
        "name":"heap_memory",
        "label":"Heap memory",
        "frequency":15,
        "metrics":
        [
            {"type":"ce_gauge_f","name":"jvmHeapMemoryUsage","unit":"bytes"}
        ]
      },
      {
        "name":"nonheap_memory",
        "label":"NonHeap memory",
        "frequency":15,
        "metrics":
        [
            {"type":"ce_gauge_f","name":"jvmNonHeapMemoryUsage","unit":"bytes"}
        ]
      },
      {
        "name":"jmxtrans_metrics",
        "label":"jmxtrans Metrics",
        "frequency":15,
        "metrics":
        [
            {"type":"ce_counter","name":"jmxtrans.jmxtrans.CollectionCount","unit":"collections"},
            {"type":"ce_gauge","name":"jmxtrans.jmxtrans.CollectionDurationInNanos","unit":"nanoseconds"},
            {"type":"ce_gauge","name":"jmxtrans.jmxtrans.ExportDurationInNanos","unit":"nanoseconds"},
            {"type":"ce_counter","name":"jmxtrans.jmxtrans.ExportCount","unit":"exports"},
            {"type":"ce_counter","name":"jmxtrans.jmxtrans.CollectedMetricsCount","unit":"metrics"},
            {"type":"ce_counter","name":"jmxtrans.jmxtrans.ExportedMetricsCount","unit":"metrics"}
         ]
      },
      {
        "name":"tomcat_global",
        "label":"Tomcat Engine",
        "frequency":15,
        "metrics":
        [
            {"type":"ce_gauge","name":"tomcat.thread-pool.currentThreadsBusy","unit":"threads"},
            {"type":"ce_gauge","name":"tomcat.thread-pool.currentThreadCount","unit":"threads"},
            {"type":"ce_counter","name":"tomcat.global-request-processor.bytesSent","unit":"bytes"},
            {"type":"ce_counter","name":"tomcat.global-request-processor.bytesReceived","unit":"bytes"},
            {"type":"ce_counter","name":"tomcat.global-request-processor.processingTime","unit":"seconds"},
            {"type":"ce_counter","name":"tomcat.global-request-processor.errorCount","unit":"errors"},
            {"type":"ce_counter","name":"tomcat.global-request-processor.requestCount","unit":"requests"}
        ]
      },  
      {
        "name":"tomcat_servlet",
        "label":"Tomcat Servlet Metrics",
        "frequency":15,
        "metrics":
        [
          {"type":"ce_counter","name":"tomcat.servlet.processingTime","unit":"seconds"},
          {"type":"ce_counter","name":"tomcat.servlet.errorCount","unit":"errors"},
          {"type":"ce_counter","name":"tomcat.servlet.requestCount","unit":"requests"}
        ]
      },
      {
        "name":"tomcat_manager",
        "label":"Tomcat Manager Metrics",
        "frequency":15,
        "metrics":
        [
          {"type":"ce_gauge","name":"tomcat.manager.activeSessions","unit":"sessions"}
        ]
      }
    ],
